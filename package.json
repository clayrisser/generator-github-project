{
  "name": "generator-github-project",
  "version": "0.0.2",
  "description": "Yeoman generator for GitHub projects",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yo",
    "yeoman",
    "git",
    "github",
    "readme",
    "changelog",
    "project",
    "license",
    "contributing"
  ],
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "build": "npm run clean && node ./node_modules/jnwb/lib/bin/nwb build && mkdir -p ./app && cp -r ./lib/* ./app",
    "clean": "rm -rf ./app/*.js ./es ./lib ./demo",
    "link": "npm link && npm link generator-readme-md",
    "lint": "node ./node_modules/eslint/bin/eslint ./",
    "lint:fix": "node ./node_modules/eslint/bin/eslint --fix ./",
    "prepublish": "npm run build",
    "start": "npm run clean && npm run lint && npm run build && node ./node_modules/yo/lib/cli --destination=demo readme-md",
    "test": "node ./node_modules/jnwb/lib/bin/nwb test",
    "test:coverage": "node ./node_modules/jnwb/lib/bin/nwb test --coverage",
    "test:watch": "node ./node_modules/jnwb/lib/bin/nwb test --server",
    "unlink": "npm unlink generator-readme-md"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "empty-dir": "^0.2.1",
    "git-user-email": "git+https://git@github.com/jamrizzi/git-user-email.git",
    "git-user-name": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "yeoman-generator": "^1.0.0",
    "yeoman-option-or-prompt": "git+https://github.com/artefact-group/yeoman-option-or-prompt.git"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^22.0.3",
    "jnwb": "^0.1.5",
    "yo": "^2.0.0"
  },
  "author": "Jam Risser <jam@jamrizzi.com>",
  "homepage": "https://github.com/jamrizzi/generator-readme-md",
  "license": "MIT",
  "repository": "https://github.com/jamrizzi/generator-readme-md"
}
